@model Doctor

<h1>Edit</h1>
<p>
	<a asp-controller="Doctores"
	   asp-action="Index">
		Back to list
	</a>
</p>

<form method="post">

	<label>Doctor</label>
	<input type="hidden" name="hospitalcod"
		   value="@Model.HospitalCod" />
	<input type="hidden" name="doctorno"
		value="@Model.DoctorNo"/>
	<label>Apellido</label>
	<input type="text" name="apellido"
		   class="form-control" value="@Model.Apellido" />
	<label>Especialidad</label>
	<input type="text" name="especialidad"
		   class="form-control" value="@Model.Especialidad" />
	<label>Salario</label>
	<input type="number" name="salario"
		   class="form-control" value="@Model.Salario" />
	<button class="btn btn-warning">
		Update
	</button>
</form>